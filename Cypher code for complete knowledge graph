// Code for creating nodes
create (MRSL:rural_highway {name:'Maximum Road Speed Limit: 80', id:'MRSL', speed:80})
create (DRCW:recommended_speed {name:'Maximum Recommended Speed: 80', id:'DRCW', speed:80, weather:'Clear Weather', roadtype:'Dry Road'})
create (WRCW:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRCW', speed:80, weather:'Clear Weather', roadtype:'Wet Road'})
create (SCRCW:recommended_speed {name:'Maximum Recommended Speed: 80', id:'SCRCW', speed:80, weather:'Clear Weather', roadtype:'Snow-Covered Road'})
create (VTRCW:recommended_speed {name:'Maximum Recommended Speed: 80', id:'VTRCW', speed:80, weather:'Clear Weather', roadtype:'Visible Tracks Road'})

create (DRLR:recommended_speed {name:'Maximum Recommended Speed: 80', id:'DRLR', speed:'80', weather:'Low Rain', roadtype:'Dry Road'})
create (WRLR:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRLR', speed:'80', weather:'Low Rain', roadtype:'Wet Road'})
create (SCRLR:recommended_speed {name:'Maximum Recommended Speed: 65', id:'SCRLR', speed:'65', weather:'Low Rain', roadtype:'Snow-Covered Road'})
create (VTRLR:recommended_speed {name:'Maximum Recommended Speed: 70', id:'VTRLR', speed:'70', weather:'Low Rain', roadtype:'Visible Tracks Road'})

create (DRMR:recommended_speed {name:'Maximum Recommended Speed: 70', id:'DRMR', speed:'70', weather:'Moderate Rain', roadtype:'Dry Road'})
create (WRMR:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRMR', speed:'80', weather:'Moderate Rain', roadtype:'Wet Road'})
create (SCRMR:recommended_speed {name:'Maximum Recommended Speed: 60', id:'SCRMR', speed:'60', weather:'Moderate Rain', roadtype:'Snow-Covered Road'})
create (VTRMR:recommended_speed {name:'Maximum Recommended Speed: 70', id:'VTRMR', speed:'70', weather:'Moderate Rain', roadtype:'Visible Tracks Road'})

create (DRHR:recommended_speed {name:'Maximum Recommended Speed: nan', id:'DRHR', speed:'nan', weather:'High Rain', roadtype:'Dry Road'})
create (WRHR:recommended_speed {name:'Maximum Recommended Speed: 75', id:'WRHR', speed:'75', weather:'High Rain', roadtype:'Wet Road'})
create (SCRHR:recommended_speed {name:'Maximum Recommended Speed: 60', id:'SCRHR', speed:'60', weather:'High Rain', roadtype:'Snow-Covered Road'})
create (VTRHR:recommended_speed {name:'Maximum Recommended Speed: 70', id:'VTRHR', speed:'70', weather:'High Rain', roadtype:'Visible Tracks Road'})

create (DRLS:recommended_speed {name:'Maximum Recommended Speed: 80', id:'DRLS', speed:'80', weather:'Low Snow', roadtype:'Dry Road'})
create (WRLS:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRLS', speed:'80', weather:'Low Snow', roadtype:'Wet Road'})
create (SCRLS:recommended_speed {name:'Maximum Recommended Speed: 65', id:'SCRLS', speed:'65', weather:'Low Snow', roadtype:'Snow-Covered Road'})
create (VTRLS:recommended_speed {name:'Maximum Recommended Speed: 70', id:'VTRLS', speed:'70', weather:'Low Snow', roadtype:'Visible Tracks Road'})

create (DRMS:recommended_speed {name:'Maximum Recommended Speed: 65', id:'DRMS', speed:'65', weather:'Moderate Snow', roadtype:'Dry Road'})
create (WRMS:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRMS', speed:'80', weather:'Moderate Snow', roadtype:'Wet Road'})
create (SCRMS:recommended_speed {name:'Maximum Recommended Speed: 65', id:'SCRMS', speed:'65', weather:'Moderate Snow', roadtype:'Snow-Covered Road'})
create (VTRMS:recommended_speed {name:'Maximum Recommended Speed: 70', id:'VTRMS', speed:'70', weather:'Moderate Snow', roadtype:'Visible Tracks Road'})

create (DRHS:recommended_speed {name:'Maximum Recommended Speed: nan', id:'DRHS', speed:'nan', weather:'High Snow', roadtype:'Dry Road'})
create (WRHS:recommended_speed {name:'Maximum Recommended Speed: 80', id:'WRHS', speed:'80', weather:'High Snow', roadtype:'Wet Road'})
create (SCRHS:recommended_speed {name:'Maximum Recommended Speed: 70', id:'SCRHS', speed:'70', weather:'High Snow', roadtype:'Snow-Covered Road'})
create (VTRHS:recommended_speed {name:'Maximum Recommended Speed: 80', id:'VTRHS', speed:'80', weather:'High Snow', roadtype:'Visible Tracks Road'})



// Code for creating relationships
match (n1:rural_highway {ID:'MRSL'}), (n2:weather_recommended_speed {ID:'MRSL'}) create (n1)-[:DryRoadClearWeather]->(n2)
match (n1:rural_highway {ID:'MRSL'}), (n2:weather_recommended_speed {ID:'DRCW'}) create (n1)-[:DryRoadLowRain]->(n2)
match (n1:rural_highway {ID:'MRSL'}), (n2:weather_recommended_speed {ID:'DRLR'}) create (n1)-[:DryRoadModerateRain]->(n2)
